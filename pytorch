pytorch

conda create -n pytorch_v2.3.1_240701

source activate pytorch_v2.3.1_240701

conda install pytorch torchvision torchaudio pytorch-cuda=11.8 -c pytorch -c nvidia
#pytorch/linux-64::pytorch-2.3.1-py3.12_cuda11.8_cudnn8.7.0_0

pip install ipykernel
python -m ipykernel install --user --name "pytorch_v2.3.1_240701" --display-name "pytorch_v2.3.1_240701"

#jupyter kernelspec uninstall pytorch_v2.3.1_240701


pip install pandas
pip install scikit-learn
